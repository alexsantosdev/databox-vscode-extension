{
  "name": "databox-vscode-extension",
  "displayName": "Databox",
  "description": "Database Client for Visual Studio Code Extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:databox-vscode-extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "webview",
          "title": "Example for webview",
          "icon": "./assets/logo.svg"
        }
      ]
    },
    "views": {
      "webview": [
        {
          "type": "webview",
          "id": "vscodeSidebar.openview",
          "name": "Webview",
          "icon": "src/assets/logo.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "databox-vscode-extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/styled-components": "^5.1.25",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/react-dom": "^18.0.6",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  }
}
